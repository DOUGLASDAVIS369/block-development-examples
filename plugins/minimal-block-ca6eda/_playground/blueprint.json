{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"landingPage": "/wp-admin/post.php?post=10&action=edit",
	"steps": [
		{
			"step": "installPlugin",
			"pluginZipFile": {
				"resource": "url",
				"url": "https://raw.githubusercontent.com/WordPress/block-development-examples/deploy/zips/minimal-block-ca6eda.zip"
			}
		},
		{
			"step": "login",
			"username": "admin",
			"password": "password"
		},
		{
			"step": "writeFile",
			"path": "/wordpress/wp-content/minimal-block-ca6eda-content-post.html",
			"data": {
				"resource": "url",
				"url": "https://raw.githubusercontent.com/WordPress/block-development-examples/trunk/plugins/minimal-block-ca6eda/_playground/minimal-block-ca6eda-content-post.html"
			}
		},
		{
			"step": "wp-cli",
			"command": "wp post create 10 --post_title='Demo minimal-block-ca6eda' --post_status='published' /wordpress/wp-content/minimal-block-ca6eda-content-post.html"
		}
	]
}
